{
  "themes": {
    "default": {
      "sections": {
        "default": {
          "palette": "regal",
          "scale": "default",
          "background": "liner",
          "typography": "default"
        },
        "profile": {
          "palette": "regal",
          "scale": "default",
          "background": "liner",
          "typography": "default"
        },
        "contact": {
          "palette": "parchment",
          "scale": "default",
          "background": "watermark",
          "typography": "default"
        },
        "article": {
          "palette": "parchment",
          "scale": "default",
          "background": "git",
          "typography": "default"
        },
        "footer": {
          "palette": "regal",
          "scale": "default",
          "background": "watermark",
          "typography": "default"
        },
        "error": {
          "palette": "regal",
          "scale": "default",
          "background": "liner",
          "typography": "default"
        }
      },
      "palettes": {
        "regal": {
          "text": "#FCFAED",
          "link": "#FF9F1C",
          "border": "#FCFAED",
          "background": "#111818",
          "highlight": "#FCFAED",
          "shadow": "#000000",
          "textShadow": "#534260",
          "accentPrimary": "#534260",
          "accentSecondary": "#FF9F1C",
          "accentShadow": "#FCFAED",
          "peach": "#EDC299",
          "brown": "#794321",
          "red": "#C5766D",
          "blue": "#336283",
          "watermark": "#00000088",
          "transparent": "#00000000"
        },
        "parchment": {
          "text": "#111818",
          "link": "#534260",
          "border": "#111818",
          "background": "#FCFAED",
          "highlight": "#FFFFFF",
          "accentPrimary": "#534260",
          "accentSecondary": "#FF9F1C",
          "accentShadow": "#111818",
          "watermark": "#FFFFFF",
          "transparent": "#00000000"
        }
      },
      "scales": {
        "default": {
          "spacing": "2rem",
          "borderRadius": "0.25rem",
          "borderWidth": "0.125rem",
          "textBody": "1.125rem",
          "textTitle": "3rem",
          "textSubtitle": "1.5rem",
          "textHeading": "2.25rem",
          "textSubHeading": "1.75rem",
          "superscript": "0.75em"
        }
      },
      "fonts": {
        "default": {
          "family": "'Public Sans', sans-serif",
          "import": "https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,100..900;1,100..900&display=swap"
        },
        "subtitle": {
          "family": "'Fragment Mono', monospace",
          "import": "https://fonts.googleapis.com/css2?family=Fragment+Mono:ital@0;1&display=swap"
        },
        "title": {
          "family": "'Atkinson Hyperlegible', sans-serif",
          "import": "https://fonts.googleapis.com/css2?family=Atkinson+Hyperlegible:ital,wght@0,400;0,700;1,400;1,700&display=swap"
        }
      },
      "backgrounds": {
        "liner": {
          "img": {
            "src": "/backgrounds/cel-tile.svg",
            "mode": "cover",
            "opacity": 0.33,
            "colourMap": {
              "logoBackground": "transparent",
              "logoCutout": "shadow",
              "logoPrimary": "shadow",
              "logoSecondary": "transparent"
            }
          },
          "fill": "background"
        },
        "git": {
          "img": {
            "src": "/backgrounds/git-timeline.svg",
            "mode": "cover",
            "opacity": 0.2,
            "colourMap": {
              "bg-primary": "accentPrimary",
              "bg-secondary": "accentSecondary",
              "bg-tertiary": "text",
              "bg-quaternary": "accentPrimary"
            }
          },
          "fill": "background"
        },
        "watermark": {
          "img": {
            "src": "/backgrounds/cel-watermark.svg",
            "mode": "cover",
            "opacity": 0.8,
            "colourMap": {
              "bgBackground": "watermark",
              "bgCutout": "background",
              "bgPrimary": "background",
              "bgSecondary": "transparent"
            }
          },
          "fill": "background"
        }
      },
      "typography": {
        "default": {
          "body": {
            "font": "default",
            "size": "textBody",
            "colour": "text"
          },
          "nav": {
            "font": "default",
            "size": "textSubtitle"
          },
          "title": {
            "font": "title",
            "size": "textTitle",
            "weight": "bold",
            "lineHeight": "4rem",
            "style": "italic",
            "colour": "text",
            "shadowColour": "textShadow"
          },
          "subtitle": {
            "font": "subtitle",
            "size": "textSubtitle",
            "lineHeight": "2.5rem",
            "style": "italic",
            "colour": "text"
          },
          "heading1": {
            "font": "title",
            "size": "textTitle",
            "weight": "bold",
            "lineHeight": "4rem",
            "colour": "text"
          },
          "heading2": {
            "font": "default",
            "size": "textHeading",
            "weight": "bold",
            "lineHeight": "3rem",
            "style": "italic",
            "colour": "text"
          },
          "heading3": {
            "font": "default",
            "size": "textSubHeading",
            "weight": "bold",
            "lineHeight": "2rem",
            "colour": "text"
          },
          "heading4": {
            "font": "default",
            "size": "text",
            "weight": "bold",
            "style": "italic",
            "colour": "text"
          },
          "tagline": {
            "font": "title",
            "size": "textSubtitle",
            "lineHeight": "2rem",
            "style": "italic",
            "colour": "text"
          },
          "listHeader": {
            "font": "default",
            "size": "textSubtitle",
            "weight": "bold",
            "lineHeight": "2.5rem",
            "colour": "text"
          },
          "listHeaderEmphasis": {
            "font": "default",
            "size": "textSubtitle",
            "weight": "bold",
            "lineHeight": "2.5rem",
            "style": "italic",
            "colour": "link",
            "shadowColour": "textShadow"
          },
          "link": {
            "style": "italic",
            "colour": "link",
            "textDecoration": "underline"
          },
          "ref": {
            "size": "superscript",
            "weight": "normal",
            "style": "normal",
            "colour": "link",
            "textDecoration": "none"
          }
        }
      },
      "graphics": {
        "default": {
          "logo": {
            "src": "/cel.svg",
            "colourMap": {
              "logoBackground": "highlight",
              "logoCutout": "accentSecondary",
              "logoPrimary": "accentPrimary",
              "logoSecondary": "accentSecondary"
            }
          },
          "avatar": {
            "src": "/avatar.svg",
            "colourMap": {
              "imgPrimary": "accentPrimary",
              "imgSecondary": "accentSecondary",
              "imgOutline": "shadow",
              "imgHighlight": "highlight",
              "imgSkin": "peach",
              "imgHair": "brown",
              "imgMouth": "red",
              "imgTeeth": "highlight",
              "imgEyes": "highlight",
              "imgIris": "blue"
            }
          },
          "titleAccent": {
            "src": "/chevron.svg",
            "colourMap": {
              "imgPrimary": "accentPrimary",
              "imgSecondary": "accentSecondary",
              "imgShadow": "accentShadow"
            }
          },
          "frame": {
            "src": "/frame.svg",
            "colourMap": {
              "imgPrimary": "accentPrimary",
              "imgSecondary": "accentSecondary",
              "imgShadow": "accentShadow"
            }
          },
          "batIcon": {
            "src": "/icons/bat.svg",
            "colourMap": {
              "iconPrimary": "accentPrimary"
            }
          },
          "emailIcon": {
